{
    "name": "dieti-deals24-frontend",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --host 0.0.0.0",
        "build": "ng build --optimization --stats-json",
        "preview": "serve -s dist/dieti-deals24-frontend/browser",
        "preview-ssl": "serve -s dist/dieti-deals24-frontend/browser -l 443 --ssl-cert server.cert --ssl-key server.key",
        "stats": "webpack-bundle-analyzer dist/dieti-deals24-frontend/stats.json",
        "zip": "zip -r dietideals24.zip dist/dieti-deals-24-frontend",
        "watch-broadcast": "ng serve --watch --configuration development --host 0.0.0.0",
        "watch": "ng serve --watch --configuration development",
        "deploy": "ng deploy --repo=https://github.com/chrscmpl/DietiDeals24-Frontend.git",
        "test": "ng test",
        "lint": "ng lint",
        "backend": "git --git-dir=../DietiDeals24-Backend/.git --work-tree=../DietiDeals24-Backend pull && docker-compose -f ../DietiDeals24-Backend/docker-compose.yaml up",
        "backend-down": "docker-compose -f ../DietiDeals24-Backend/docker-compose.yaml down -v",
        "backend-newdb": "npm run backend-down && rimraf ../DietiDeals24-Backend/postgres_volume && npm run backend",
        "db": "docker-compose -f ../DietiDeals24-Backend/docker-compose.yaml exec database psql -d dietideals24 -U postgres -p 5433"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.3.10",
        "@angular/cdk": "^17.3.8",
        "@angular/common": "^17.3.10",
        "@angular/compiler": "^17.3.10",
        "@angular/core": "^17.3.10",
        "@angular/forms": "^17.3.10",
        "@angular/platform-browser": "^17.3.10",
        "@angular/platform-browser-dynamic": "^17.3.10",
        "@angular/router": "^17.3.10",
        "@angular/service-worker": "^17.3.10",
        "browser-image-compression": "^2.0.2",
        "hammerjs": "^2.0.8",
        "iban-ts": "^0.4.0",
        "lodash-es": "^4.17.21",
        "normalize-scss": "^8.0.0",
        "primeicons": "^7.0.0",
        "primeng": "^17.16.0",
        "rxjs": "~7.8.0",
        "ts-cacheable": "^1.0.10",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.0.0",
        "@angular-eslint/builder": "17.3.0",
        "@angular-eslint/eslint-plugin": "17.3.0",
        "@angular-eslint/eslint-plugin-template": "17.3.0",
        "@angular-eslint/schematics": "17.3.0",
        "@angular-eslint/template-parser": "17.3.0",
        "@angular/cli": "^17.0.0",
        "@angular/compiler-cli": "^17.3.10",
        "@types/jasmine": "~5.1.0",
        "@types/lodash-es": "^4.17.12",
        "@typescript-eslint/eslint-plugin": "7.2.0",
        "@typescript-eslint/parser": "7.2.0",
        "angular-cli-ghpages": "^2.0.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "gzipper": "^7.2.0",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "prettier": "^3.2.5",
        "prettier-eslint": "^16.3.0",
        "rimraf": "^6.0.1",
        "serve": "^14.2.3",
        "typescript": "~5.2.2"
    }
}
